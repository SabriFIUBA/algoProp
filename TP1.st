io y:otroPrecio	^ ( precio between: unPrecio  and: otroPrecio  )! !Inmueble subclass: #Casa	instanceVariableNames: 'tipo superficie ambientes pisos'	classVariableNames: ''	poolDictionaries: ''	category: 'TP1'!!Casa methodsFor: 'as yet unclassified' stamp: 'Sabrina 9/17/2016 21:20'!iniciarEnBarrio: unBarrio conPrecio: unPrecio conMoneda: unaMoneda conSuperficie: unaSuperficie conAmbientes: cantidadAmbientes conPisos: cantidadPisos	barrio := unBarrio .	precio := unPrecio .	moneda := unaMoneda .	superficie := unaSuperficie .	ambientes := cantidadAmbientes .	pisos := cantidadPisos .! !!Casa methodsFor: 'as yet unclassified' stamp: 'Sabrina 9/17/2016 21:17'!initialize	tipo := 'casa' .! !!Casa methodsFor: 'as yet unclassified' stamp: 'Sabrina 9/20/2016 13:56'!miSuperficieEstaEntre: unValor y: otroValor	^ superficie between: unValor and: otroValor! !Inmueble subclass: #Cochera	instanceVariableNames: 'tipo'	classVariableNames: ''	poolDictionaries: ''	category: 'TP1'!!Cochera methodsFor: 'as yet unclassified' stamp: 'Sabrina 9/17/2016 21:24'!iniciarEnBarrio: unBarrio conPrecio: unPrecio conMoneda: unaMoneda	barrio := unBarrio .	precio := unPrecio .	moneda := unaMoneda .! !!Cochera methodsFor: 'as yet unclassified' stamp: 'Sabrina 9/17/2016 21:17'!initialize	tipo := 'cochera' .! !Inmueble subclass: #Departamento	instanceVariableNames: 'tipo superficie expensas ambientes'	classVariableNames: ''	poolDictionaries: ''	category: 'TP1'!!Departamento methodsFor: 'as yet unclassified' stamp: 'Sabrina 9/17/2016 21:23'!iniciarEnBarrio: unBarrio conPrecio: unPrecio conMoneda: unaMoneda conSuperficie: unaSuperficie conExpensas: valorExpensas conAmbientes: cantidadAmbientes	barrio := unBarrio .	precio := unPrecio .	moneda := unaMoneda .	superficie := unaSuperficie .	expensas := valorExpensas .	ambientes := cantidadAmbientes .! !!Departamento methodsFor: 'as yet unclassified' stamp: 'Sabrina 9/20/2016 15:01'!misExpensasEstanEntre:unMonto y:otroMonto	^ expensas between: unMonto  and: otroMonto ! !!Departamento methodsFor: 'as yet unclassified' stamp: 'Sabrina 9/17/2016 21:17'!initialize	tipo := 'departamento' .! !!Departamento methodsFor: 'as yet unclassified' stamp: 'Sabrina 9/20/2016 04:14'!miSuperficieEstaEntre: unValor y: otroValor	^ superficie between: unValor and: otroValor! !Object subclass: #Publicacion	instanceVariableNames: 'codigo propiedad inmobiliaria'	classVariableNames: ''	poolDictionaries: ''	category: 'TP1'!!Publicacion methodsFor: 'as yet unclassified' stamp: 'Sabrina 9/20/2016 13:51'!miPropiedadEstaEnElBarrio: unBarrio	^ propiedad meUbicoEnElBarrio: unBarrio.! !!Publicacion methodsFor: 'as yet unclassified' stamp: 'Sabrina 9/20/2016 15:04'!miPropiedadTieneExpensasEntre: unMonto y: otroMonto	^ propiedad misExpensasEstanEntre: unMonto y: otroMonto ! !!Publicacion methodsFor: 'as yet unclassified' stamp: 'Sabrina 9/20/2016 14:21'!miPropiedadTieneMoneda: unaMoneda	^ propiedad miMonedaEs: unaMoneda ! !!Publicacion methodsFor: 'as yet unclassified' stamp: 'Sabrina 9/20/2016 17:07'!miPropiedadEs: unInmueble	^ propiedad soy: unInmueble .! !!Publicacion methodsFor: 'as yet unclassified' stamp: 'Sabrina 9/20/2016 14:20'!miPropiedadTienePrecioEntre: unPrecio y:otroPrecio	^ propiedad miPrecioEstaEntre: unPrecio  y: otroPrecio ! !!Publicacion methodsFor: 'as yet unclassified' stamp: 'Sabrina 9/20/2016 14:19'!miPropiedadTieneSuperficieEntre: unaSuperficie y:otraSuperficie	^ propiedad miSuperficieEstaEntre: unaSuperficie y:otraSuperficie ! !!Publicacion methodsFor: 'as yet unclassified' stamp: 'Sabrina 9/20/2016 15:28'!obtenerPrecioDeLaPropiedad	^ propiedad precioPropiedad ! !Publicacion subclass: #PublicacionPorDia	instanceVariableNames: 'inicio fin'	classVariableNames: ''	poolDictionaries: ''	category: 'TP1'!!PublicacionPorDia methodsFor: 'as yet unclassified' stamp: 'Sabrina 9/17/2016 21:04'!codigoPublicacion	^codigo .! !!PublicacionPorDia methodsFor: 'as yet unclassified' stamp: 'Sabrina 9/20/2016 15:56'!calcularCostoPublicacion	| cambioAPesos dias precio costo |	dias := (fin - inicio) days + 1.	cambioAPesos := (self miPropiedadTieneMoneda: 'Dolar')		ifTrue: [ cambioAPesos := 15 ]		ifFalse: [ cambioAPesos := 1 ].	precio := self obtenerPrecioDeLaPropiedad.	costo := dias * cambioAPesos * precio / 1000000.	^ costo! !!PublicacionPorDia methodsFor: 'as yet unclassified' stamp: 'Sabrina 9/20/2016 16:41'!cantidadDeDiasParaMes: unaFechaStringConAnioMes 	| anio mes diasDeMes diasPublicacion currentDate |	anio := (unaFechaStringConAnioMes copyFrom: 1 to: 4) asInteger.	mes := (unaFechaStringConAnioMes copyFrom: 6 to: 7) asInteger.	diasDeMes := Date daysInMonth: mes forYear: anio.	diasPublicacion := 0.	currentDate := Date year: anio month: mes day: 1.	1 to: diasDeMes do: [ :eachDay | 		(currentDate between: inicio and: fin)			ifTrue: [ diasPublicacion := diasPublicacion + 1 ].		currentDate := currentDate addDays: 1 ].^ diasPublicacion ! !!PublicacionPorDia methodsFor: 'as yet unclassified' stamp: 'Sabrina 9/20/2016 16:44'!calcularCostoPublicacionParaMes: unaFechaStringConAnioMes	| costo cambioAPesos dias |	dias := self cantidadDeDiasParaMes: unaFechaStringConAnioMes.	(cambioAPesos := self miPropiedadTieneMoneda: 'Dolar')		ifTrue: [ cambioAPesos := 15 ]		ifFalse: [ cambioAPesos := 1 ].	costo := dias * propiedad precioPropiedad * cambioAPesos / 1000000.	^ costo! !!PublicacionPorDia methodsFor: 'as yet unclassified' stamp: 'Sabrina 9/20/2016 15:48'!iniciarConCodigo: unCodigo conPropiedad: unaPropiedad deInmobiliaria: unaInmobiliaria desde: unaFecha hasta: otraFecha	codigo := unCodigo.	propiedad := unaPropiedad.	inmobiliaria := unaInmobiliaria.	inicio := self parsearFecha: unaFecha.	fin := self parsearFecha: otraFecha.	! !!PublicacionPorDia methodsFor: 'as yet unclassified' stamp: 'Sabrina 9/19/2016 15:40'!parsearFecha: unaFechaString	| fecha anio mes dia |	anio := (unaFechaString copyFrom: 1 to: 4) asInteger  .	mes := (unaFechaString copyFrom: 6 to: 7) asInteger  .	dia := (unaFechaString copyFrom: 9 to: 10)  asInteger  .	fecha := Date year: anio month: mes day: dia .		^fecha.! !Publicacion subclass: #PublicacionPorMes	instanceVariableNames: 'mes anio'	classVariableNames: ''	poolDictionaries: ''	category: 'TP1'!!PublicacionPorMes methodsFor: 'as yet unclassified' stamp: 'Sabrina 9/20/2016 16:17'!iniciarConCodigo: unCodigo conPropiedad: unaPropiedad deInmobiliaria: unaInmobiliaria enMes: unaFechaConAnioMes	codigo := unCodigo.	propiedad := unaPropiedad.	inmobiliaria := unaInmobiliaria.	self parsearFecha: unaFechaConAnioMes! !!PublicacionPorMes methodsFor: 'as yet unclassified' stamp: 'Sabrina 9/20/2016 15:55'!calcularCostoPublicacion	| cambioAPesos precio costo |	cambioAPesos := (self miPropiedadTieneMoneda: 'Dolar')		ifTrue: [ cambioAPesos := 15 ]		ifFalse: [ cambioAPesos := 1 ].	precio := self obtenerPrecioDeLaPropiedad.	costo := 25 * cambioAPesos * precio / 1000000.	^ costo! !!PublicacionPorMes methodsFor: 'as yet unclassified' stamp: 'Sabrina 9/17/2016 21:04'!codigoPublicacion	^codigo .! !!PublicacionPorMes methodsFor: 'as yet unclassified' stamp: 'Sabrina 9/20/2016 16:13'!calcularCostoPublicacionParaMes: unaFechaStringConAnioMes	| anioPedido mesPedido costo |	costo := 0.	anioPedido := (unaFechaStringConAnioMes copyFrom: 1 to: 4) asInteger.	mesPedido := (unaFechaStringConAnioMes copyFrom: 6 to: 7) asInteger.	(anioPedido = anio) & (mesPedido = mes)		ifTrue: [ costo := self calcularCostoPublicacion ].	^ costo! !!PublicacionPorMes methodsFor: 'as yet unclassified' stamp: 'Sabrina 9/19/2016 15:52'!parsearFecha: unaFechaStringConAnioMes	anio := (unaFechaStringConAnioMes copyFrom: 1 to: 4) asInteger.	mes := (unaFechaStringConAnioMes copyFrom: 6 to: 7) asInteger! !